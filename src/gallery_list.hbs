{{#each this}}
<a class="photo-card">
    
    <div class="info">
        <img src="{{this.poster_path}}" alt="{{this.title}}" width="200" height="100" load="lazy">
        <p class="info-item">
            <b>Name {{this.original_title}}</b>
        </p>
        <p class="info-item">
            <b>Popularity {{this.popularity}}</b>
        </p>
        <p class="info-item">
            <b>Comments {{this.overview}}</b>
        </p>
        <p class="info-item">
            <b>client_id <span class="client_id">{{this.id}}</span></b>
        </p>
        
    <button class="btn" type="button" id = {{this.id}}>Queue</button>
    <button class="btn" type="button" id = {{this.id}}>Watched</button>
    </div>
</a>
{{/each}}